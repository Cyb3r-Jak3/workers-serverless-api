{
    "name": "serverless-api",
    "version": "1.12.2",
    "description": "Serverless version of my API",
    "scripts": {
        "dev": "wrangler dev --test-scheduled",
        "dev:remote": "wrangler dev --remote",
        "format": "prettier --write *.{json,js,ts} src/**/*.{js,ts} ",
        "test:lint": "eslint --max-warnings=0 src && prettier --check *.{json,js,ts} src/**/*.{js,ts} --list-different",
        "test:unit": "vitest run",
        "test": "pnpm run test:lint && npm run test:unit"
    },
    "author": "cyb3r-jak3",
    "license": "MIT",
    "eslintConfig": {
        "root": true,
        "extends": [
            "typescript",
            "prettier"
        ]
    },
    "devDependencies": {
        "@cloudflare/vitest-pool-workers": "^0.2.2",
        "@cloudflare/workers-types": "^4.20240129.0",
        "@openpgp/web-stream-tools": "^0.1.1",
        "@types/node": "20.8.3",
        "@types/openpgp": "^4.4.18",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.7.0",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-typescript": "^3.0.0",
        "prettier": "^3.2.4",
        "typescript": "^5.4.5",
        "vitest": "1.3.0",
        "wrangler": "^3.51.2"
    },
    "dependencies": {
        "@cyb3r-jak3/workers-common": "^5.4.1",
        "@octokit/core": "^5.0.1",
        "domhandler": "^5.0.3",
        "hono": "^4.2.5",
        "htmlparser2": "^9.0.0",
        "openpgp": "^5.11.1"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.9.5"
    },
    "overrides": {
        "@types/node": "20.8.3"
    },
    "pnpm": {
        "override": {
            "@types/node": "20.8.3"
        }
    }
}
