{
    "name": "serverless-api",
    "version": "1.16.0",
    "description": "Serverless version of my API",
    "scripts": {
        "dev": "wrangler dev --test-scheduled",
        "dev:remote": "wrangler dev --remote",
        "format": "prettier --write *.{json,js,ts} src/**/*.{js,ts} ",
        "test:lint": "eslint --max-warnings=0 src && prettier --check *.{json,js,ts} src/**/*.{js,ts}",
        "test:unit": "vitest run",
        "test": "pnpm run test:lint && npm run test:unit"
    },
    "author": "cyb3r-jak3",
    "license": "MIT",
    "eslintConfig": {
        "root": true,
        "extends": [
            "typescript",
            "prettier"
        ]
    },
    "devDependencies": {
        "@cloudflare/vitest-pool-workers": "^0.8.0",
        "@cloudflare/workers-types": "^4.20250327.0",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.23.0",
        "@openpgp/web-stream-tools": "^0.1.3",
        "@types/node": "22.15.3",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint": "^9.0.0",
        "eslint-config-prettier": "^10.0.0",
        "eslint-config-typescript": "^3.0.0",
        "globals": "^16.0.0",
        "prettier": "^3.5.3",
        "typescript": "^5.8.2",
        "vitest": "~3.0.9",
        "wrangler": "4.15.2"
    },
    "dependencies": {
        "@cyb3r-jak3/workers-common": "^5.4.1",
        "@microlabs/otel-cf-workers": "1.0.0-rc.49",
        "@octokit/core": "^6.1.4",
        "domhandler": "^5.0.3",
        "hono": "^4.7.5",
        "htmlparser2": "^10.0.0",
        "openpgp": "^6.1.1"
    }
}
