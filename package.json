{
    "name": "serverless-api",
    "version": "1.15.0",
    "description": "Serverless version of my API",
    "scripts": {
        "dev": "wrangler dev --test-scheduled",
        "dev:remote": "wrangler dev --remote",
        "format": "prettier --write *.{json,js,ts} src/**/*.{js,ts} ",
        "test:lint": "eslint --max-warnings=0 src && prettier --check *.{json,js,ts} src/**/*.{js,ts}",
        "test:unit": "vitest run",
        "test": "pnpm run test:lint && npm run test:unit"
    },
    "author": "cyb3r-jak3",
    "license": "MIT",
    "eslintConfig": {
        "root": true,
        "extends": [
            "typescript",
            "prettier"
        ]
    },
    "devDependencies": {
        "@cloudflare/vitest-pool-workers": "^0.4.27",
        "@cloudflare/workers-types": "^4.20240925.0",
        "@openpgp/web-stream-tools": "^0.1.3",
        "@types/node": "22.0.2",
        "@types/openpgp": "^4.4.22",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-typescript": "^3.0.0",
        "prettier": "^3.3.0",
        "typescript": "^5.5.3",
        "vitest": "1.3.0",
        "wrangler": "^3.80.3"
    },
    "dependencies": {
        "@cyb3r-jak3/workers-common": "^5.4.1",
        "@microlabs/otel-cf-workers": "1.0.0-rc.45",
        "@octokit/core": "^6.1.2",
        "domhandler": "^5.0.3",
        "hono": "^4.6.5",
        "htmlparser2": "^9.1.0",
        "openpgp": "^5.11.2"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.23.0"
    },
    "overrides": {
        "@types/node": "20.8.3"
    },
    "pnpm": {
        "override": {
            "@types/node": "20.8.3"
        }
    }
}
