{
    "name": "serverless-api",
    "version": "1.12.2",
    "description": "Serverless version of my API",
    "scripts": {
        "dev": "wrangler dev --test-scheduled",
        "dev:remote": "wrangler dev",
        "format": "prettier --write *.{json,js} src/**/*.{js,ts} ",
        "test:lint": "eslint --max-warnings=0 src && prettier --check *.{json,js} src/**/*.{js,ts}",
        "test:unit": "vitest run",
        "test": "npm run test:lint && npm run test:unit"
    },
    "author": "cyb3r-jak3",
    "license": "MIT",
    "eslintConfig": {
        "root": true,
        "extends": [
            "typescript",
            "prettier"
        ]
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20240129.0",
        "@openpgp/web-stream-tools": "^0.0.13",
        "@types/openpgp": "^4.4.18",
        "@typescript-eslint/eslint-plugin": "^6.9.1",
        "@typescript-eslint/parser": "^6.20.0",
        "@vitest/coverage-c8": "^0.33.0",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-typescript": "^3.0.0",
        "prettier": "^3.2.4",
        "service-worker-mock": "^2.0.5",
        "ts-loader": "^9.5.1",
        "typescript": "^5.3.3",
        "vitest": "^0.34.6",
        "vitest-environment-miniflare": "^2.14.2",
        "wrangler": "^3.19.0"
    },
    "dependencies": {
        "@cyb3r-jak3/workers-common": "^5.4.1",
        "@octokit/core": "^5.0.1",
        "domhandler": "^5.0.3",
        "hono": "^4.0.8",
        "htmlparser2": "^9.0.0",
        "openpgp": "^5.10.1"
    }
}
