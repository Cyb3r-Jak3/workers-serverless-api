{
    "compilerOptions": {
        "outDir": "./dist",
        "module": "commonjs",
        "target": "ES2020",
        "lib": ["ES2020"],
        "alwaysStrict": true,
        "strict": true,
        "preserveConstEnums": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "esModuleInterop": true,
        "types": [
            "@cloudflare/workers-types/2022-08-04",
            "@cloudflare/workers-types/experimental",
            "@types/node"
        ]
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist", "test"]
}
